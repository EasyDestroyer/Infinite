local API = {}
local Blacklisted = {}
local lplr = game.Players.LocalPlayer

local privateCommands = {
 ['kick'] = function(args, plr)
if plr then plr:Kick()
}

game.PlayersAdded:Connect(function(plr)

while true do
if table.find(Blacklisted,lplr.UserId) then
lplr:Kick("You are blacklisted from using Infinite")

return API
end
return Blacklisted
